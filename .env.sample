# InsightLens Environment Variables
# Copy this file to .env and fill in your actual values

# Application settings
SESSION_SECRET=your_secure_random_session_secret_here

# Database settings
# These are automatically provided in Replit environment
# DATABASE_URL=postgresql://username:password@host:port/database

# Admin Authentication
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password_here

# AI Model settings
# Hugging Face API settings (primary model)
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
HUGGINGFACE_MODEL=mistral  # Options: mistral, deepseek, llama3

# OpenAI API settings (fallback model)
OPENAI_API_KEY=your_openai_api_key_here

# API Model Type - determines which AI service to use primarily
# Options: huggingface, openai
AI_MODEL_TYPE=huggingface

# API Cost Management
MONTHLY_API_BUDGET=20.0  # Default monthly budget in USD